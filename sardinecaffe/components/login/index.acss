page{
  overflow:hidden;
  width: 100%;
  height: 100%;
  background-size:100% 100%;
  position: relative;
}
.bg_image{
  position: fixed;
  bottom: 186rpx;
  left: 284rpx;
  width: 182rpx;
  height: 142rpx;
 background: url(data:image/png;base64,) center center no-repeat;
 background-size: 182rpx 142rpx;
}
.wrap_login{
  width: 560rpx;
  height: 368rpx;
  background:#fff;
  position:absolute;
  border-radius:32rpx;
  top:356rpx;
  left:96rpx;
  overflow: hidden;
  box-shadow:0 0 4px 1px rgba(220,220,220,0.50);
  }
.text_area{
  width: 464rpx;
  height: 144rpx;
  font-size: 32rpx;
  margin: 0 auto;
  margin-top: 72rpx;
  text-align: center;
  color: #333;
}
.set_btn{
  box-sizing: border-box;
  position: absolute;
  bottom: 0rpx;
  width: 100%;
  height: 104rpx;
  font-size: 36rpx;
  text-align: center;
  border-top: 1px solid #f2f2f2;
}
.set_btn button{
  width: 100%;
  height: 100%;
  font-size:28rpx;
  margin:0;
  line-height: 104rpx;
  border-radius:0px;
  background-color:#fff;
  box-shadow: none;
  border:none;
}
.btn_login{
  color: #009fe8;
}
button:after{
  border:none;
}

