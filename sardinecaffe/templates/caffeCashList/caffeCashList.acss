.wrap {
  width: 100%;
  overflow-x: hidden;
  padding-bottom: 170rpx;
}

.coffee_list, .crash_list, .light_food, .lose_list {
  padding: 40rpx 30rpx;
  overflow: hidden;
}

.lose_list {
  filter: gray;
}

.list {
  position: relative;
  width: 100%;
  height: 220rpx;
  margin-top: 20rpx;
  font-size: 34rpx;
  overflow: hidden;
  border-radius: 32rpx;
  box-shadow: 0 0 8rpx 0 rgba(0, 0, 0, 0.20);
  z-index: 3;
}

.list:first-child {
  margin-top: 0rpx;
}

.listCon {
  display: flex;
  flex-direction: row;
  height: 100%;
  width: 100%;
  background: #fff;
  position: absolute;
  transition: all 0.4s ease;
  -webkit-transition: all 0.4s ease;
  z-index: 2;
  overflow: hidden;
}

.send_share {
  position: absolute;
  height: 220rpx;
  width: 160rpx;
  line-height: 220rpx;
  text-align: center;
  right: 0rpx;
  top: 0rpx;
  background: #999;
  color: #fff;
  z-index: 1;
  -webkit-transform: translateX(160rpx);
  transform: translateX(160rpx);
  -webkit-transition: all 0.4s ease;
  transition: all 0.4s ease;
}

.touch-move-active .send_share {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

.touch-move-active .listCon {
  -webkit-transform: translateX(-160rpx);
  transform: translateX(-160rpx);
}

.sawtooth {
  position: absolute;
  width: 40rpx;
  height: 100%;
  top: 0rpx;
  left: 198rpx;
  text-align: center;
}

.sawtooth text {
  display: block;
  width: 2rpx;
  height: 12rpx;
  border-radius: 4rpx;
  background: #f2f2f2;
  margin-top: 8rpx;
  margin-left: 17rpx;
}

.sawtooth text:first-child {
  margin-top: 4rpx;
}

.sawtooth text:last-child {
  margin-top: 4rpx;
}

.list_loge {
  width: 216rpx;
  height: 100%;
  text-align: center;
}

.image_loge {
  width: 200rpx;
  height: 200rpx;
  margin-top: 10rpx;
}

.image_loge2 {
  width: 100%;
  height: 100%;
}

.gray {
  -webkit-filter: grayscale(100%);
  -moz-filter: grayscale(100%);
  -ms-filter: grayscale(100%);
  -o-filter: grayscale(100%);
  filter: gray;
}

.graycolor {
  color: gray !important;
}

.graybgcolor {
  background: gray !important;
}

.list_text {
  flex: 1;
  height: 100%;
  border-radius: 32rpx;
  position: relative;
}

.list_imgae {
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: absolute;
}

.list_imgae image {
  width: 100%;
  height: 100%;
}

.list_up_dowm {
  display: flex;
  flex-direction: column;
  position: absolute;
  width: 100%;
}

.list_up {
  height: 64rpx;
  position: relative;
}

.list_up_2 {
  background: #db1e35;
}

.list_up_3 {
  background: gray;
}

.up_bg {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}

.list_up_img {
  width: 100%;
  height: 100%;
}

.list_up_bg {
  position: absolute;
  height: 64rpx;
  width: 100%;
  z-index: 444;
  background: url(data:image/png;base64,) right center no-repeat;
  background-position-x: 180rpx;
  background-size: 264rpx 28rpx;
}

.list_dowm {
  display: flex;
  flex-direction: row;
  background: #fff;
  height: 164rpx;
  width: 100%;
}

.list_left, .lightfood_left {
  width: 194rpx;
}

.lightfood_left {
  padding-left: 40rpx;
}

.list_right{
  flex: 1;
  margin-right: 29rpx;
}

.lightfood_right {
  flex: 1;
  margin-right: 40rpx;
  text-align: right;
  font-weight: bold;
}
.lightname {
  margin-top: 40rpx;
  font-size: 34rpx;
  color: #333;
}
.lightlimit {
  margin-top: 18rpx;
  font-size: 24rpx;
  color: #333;
}
.now_use {
  width: 142rpx;
  height: 36rpx;
  border: 2rpx solid #db1e35;
  box-sizing: border-box;
  border-radius: 30rpx;
  font-size: 20rpx;
  color: #db1e35;
  text-align: center;
  line-height: 36rpx;
  margin-top: 48rpx;
}
.lightovertime {
  font-size: 16rpx;
  color: #999;
  margin-top: 18rpx;
}
.listCon .cn {
  display: block;
  width: 100%;
  color: #333;
  font-weight: bold;
  text-align: right;
  margin-top: 32rpx;
  font-size: 34rpx;
}

.listCon .en {
  display: block;
  width: 100%;
  font-size: 30rpx;
  text-align: right;
  font-weight: bold;
  opacity: 0.6;
  color: #333;
  margin-top: 14rpx;
}

.time {
  display: block;
  opacity: 0.6;
  color: #999;
  font-size: 16rpx;
  margin-top: 94rpx;
  margin-left: 22rpx;
}

.btn navigator, .gzTip {
  text-align: center;
  display: block;
  font-size: 26rpx;
  color: #999;
  padding: 0 0 10px 0;
}

.btn navigator:active, .btn navigator:hover {
  background: none;
}

.check label.active:before {
  content: '';
  display: block;
  width: 0;
  height: 0;
  border-color: transparent #fff #fff transparent;
  border-style: solid;
  border-width: 12px 2px 2px 5px;
  transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  position: absolute;
  left: 9px;
  top: 3px;
}

.noneIMg {
  width: 304rpx;
  height: 196rpx;
  display: block;
  margin: 50px auto 540rpx auto;
  z-index: 50;
}

.noneList {
  margin-top: 20rpx;
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #ccc;
}

.btn {
  width: 100%;
  padding: 12px 14px;
  position: fixed;
  left: 0px;
  bottom: 0px;
  z-index: 90;
  box-sizing: border-box;
  background: #fff;
}

.btnIpx {
  padding: 24rpx 28rpx 58rpx 28rpx;
}

.btnIpx2 {
  padding: 24rpx 28rpx 92rpx 28rpx;
}

.btn view {
  display: block;
  margin: 0 auto;
  width: 690rpx;
  height: 38px;
  line-height: 38px;
  text-align: center;
  color: #08a5e1;
  border: 1px solid #08a5e1;
  font-size: 34rpx;
  border-radius: 4px;
  position: relative;
  z-index: 60;
}

.btn.active view {
  background: #08a5e1;
  color: #fff;
}

.btn view:active, .btn view.active {
  background: #08a5e1;
  color: #fff;
}

.mask {
  opacity: 0;
  visibility: hidden;
  transition: all 0.5s ease;
  position: fixed;
  left: 0;
  top: 0px;
  width: 100%;
  height: 100%;
  z-index: 999;
  background: rgba(0, 0, 0, 0.6);
}

.mask.show {
  opacity: 1;
  visibility: initial;
}

.maskCon {
  top: 50%;
  transform: translate3d(0, -50%, 0);
  position: absolute;
  font-size: 36rpx;
  color: #08a5e1;
  text-align: center;
  width: 70%;
  left: 15%;
  box-sizing: border-box;
  padding: 25px;
  background: #fff;
  border-radius: 10px;
}

.maskCon text:first-child {
  margin-top: 14px;
}

.maskCon text {
  display: block;
  color: #555;
  font-size: 30rpx;
  margin-top: 6px;
}

.tabBar_pos {
  width: 100%;
}

.tabBar {
  display: flex;
  font-size: 28rpx;
  flex-direction: row;
  height: 88rpx;
  line-height: 88rpx;
  background: #fff;
}

.tabOne {
  flex: 1;
  text-align: center;
}

.tabTwo {
  flex: 1;
  text-align: center;
}

.tabThree {
  flex: 1;
  text-align: center;
}

.tabFour {
  flex: 1;
  text-align: center;
}

.tabBar text {
  width: 102rpx;
}

.tabBar text.active {
  border-bottom: 4rpx solid #db1e35;
  font-size: 34rpx;
  color: #db1e35;
  font-weight: bold;
  padding-bottom: 10rpx;
}

.coupon_list {
  overflow: hidden;
}

.hidden {
  display: none;
}

.show {
  display: block;
}

.clearcss {
  float: none;
  margin: 0 auto;
  width: 690rpx;
}

.price_coupon {
  margin-top: 60rpx;
  font-size: 64rpx;
  color: #ba1c22;
}

.sign_coupon {
  font-size: 60rpx;
  color: #db1e35;
  line-height: 60rpx;
  font-weight: bold;
}

.clearbg {
  font-size: 96rpx;
  color: #db1e35;
  line-height: 96rpx;
  font-weight: bold;
}

.lightfood_coupon {
  color: #333;
  display: block;
  text-align: center;
  margin-top: 42rpx;
}

.lightfood_sign {
  font-size: 40rpx;
  display: inline-block;
  font-family: 'DIN Condensed';
  vertical-align: top;
}

.lightfood_price {
  vertical-align: top;
  font-size: 160rpx;
  font-family: 'DIN Condensed';
}
.nameMargin{
margin-top: 105rpx;
}
