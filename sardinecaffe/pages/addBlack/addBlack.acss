
page{background: #f2f2f2;padding-bottom: 130rpx;}
.searchBox{
	position: relative;
	padding:10px 79px 10px 14px;
}
.searchBox view{
	background: url(data:image/png;base64,) #fff 10px center no-repeat;
	background-size: 32rpx 32rpx;
	position: relative;
	border-radius: 3px;overflow: hidden;
}
.searchBox input{
	font-size: 32rpx;
	height: 75rpx;
	padding: 0px 10px 0px 30px;
}
.searchBox text{ width: 56px;color: #08a5e1; height: 75rpx;background: #fff; border-radius: 4px;font-size: 32rpx; display: block; position: absolute; right: 14px; top: 10px; text-align: center; line-height: 75rpx; }
.title{color: #888;font-size: 35rpx; padding: 0px 10px 5px 14px;}
.item{padding: 12px 14px 12px 50px;background: #fff;overflow: hidden; position: relative; border-bottom: 1px solid #eee}
.item image.userImg{float: left; vertical-align: top;}
.userInfo view{font-size: 30rpx;width: 100%; float: left; font-weight: bold;}
.userInfo {overflow: hidden; padding-left: 11px;}
.n{margin-top: 1px;}
.n image{width: 24rpx; height: 29rpx; position: relative; top: 2px;}
.levBox{margin-top: -1px;}
.levBox image{width: 22rpx; height: 22rpx;margin-right: 4px;}
.btn{
	font-size: 28rpx;
	width: 120rpx; padding: 1px 0; border-radius: 3px; text-align: center;
	color: #08a5e1;border:1px solid #08a5e1;
	position: absolute; right: 14px; top: 50%;
	transform: translate3d(0,-50%,0);
}
.btn.active{background: #08a5e1;color: #fff}

.searchBox view.close{ position: absolute; right: 0px; height: 100%;width: 40px; top: 0px; background: url(data:image/png;base64,) center no-repeat;
background-size: 38rpx 38rpx;z-index: 50
 }

.item checkbox { position: absolute; left: 13px; top: 50%;
	transform: translate3d(0,-50%,0);border-radius: 50%;
 }
.item .wx-checkbox-input:after{
	border-radius: 50%;
}
.fixedBtn{width: 100%; position: fixed; background: #fff;z-index: 190; left: 0px;bottom: 0px; padding: 10px 0;box-shadow: 0px -2px 5px rgba(0,0,0,.08)}
.btn2{background: #08a5e1;width: 90%;margin: 0 auto; height: 90rpx; color: #fff;  line-height: 90rpx; border-radius: 4px; text-align: center;}
.btn2:active{background: #1088b6;}
