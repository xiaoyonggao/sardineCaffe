}

.good_text view {
  height: 40rpx;
  line-height: 40rpx;
  margin-bottom: 14rpx;
  font-size: 28rpx;
  color: #333;
  
}
.good_name {
  margin-top: 4rpx;
  width: 465rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
 }

.good_num {
  font-size: 28rpx;
  color: #999 !important;
}

