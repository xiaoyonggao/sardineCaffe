shared-checkbox{
  border: none;
}
.wrap {
  padding: 36rpx 50rpx;
}
.data_num {
  display: flex;
  flex-direction: row;
  margin-bottom:28rpx;
}

.order_data {
  flex: 1;
  text-align: left;
  font-size: 24rpx;
  color: #666;
}

.order_num {
  flex: 1;
  font-size: 24rpx;
  color: #666;
  text-align: right;
}

.checkbox_group {
  overflow: hidden;
}

.checkbox_in {
  padding: 24rpx  0rpx 30rpx 0rpx;
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid #f2f2f2;
}

.checkbox_left {
  overflow: hidden;
  height:160rpx;
}

.checkbox_right {
  flex: 1;
}

.checkbox_out {
  position: relative;
  width: 76rpx;
  height: 76rpx;
  display: inline-block;
  vertical-align: middle;
}
checkbox[hidden]{
  display: none;
}
.box_label {
  display: inline-block;
  box-sizing: border-box;
  width: 42rpx;
  height: 42rpx;
  border-radius: 36rpx;
  border: 1px solid #dedede;
  position: absolute;
  top: 8px;
  left: 1px;
  right: 0px;
  bottom: 0px;
}
.active{
 position: absolute;
 border:1px solid #db1e35;
 background: url(data:image/png;base64,) 8rpx 12rpx no-repeat;
 background-size:24rpx 16rpx;
}

.goods_img {
  vertical-align: middle;
  display: inline-block;
  width:160rpx;
  height:160rpx;
}
.goods_img image{
  width: 100%;
  height: 100%;
}
.goods_dec {
  display: flex;
  flex-direction: column;
  text-align: right;
}

.goods_name {
  font-size: 30rpx;
  color: #333;
  margin-top:20rpx;
  text-overflow:ellipsis;
  overflow:hidden;
  white-space: nowrap;
  width: 410rpx;
}

.goods_price {
  font-size: 30rpx;
  color: #333;
  margin-top: 6rpx;
}

.goods_price_sign {
  font-size: 20rpx;
  color: #333;
}

.goods_num {
  overflow: hidden;
  position: relative;
  text-align: right;
  height:55rpx;
}

.reduce_num {
  position: absolute;
   box-sizing: border-box;
  width: 32rpx;
  height: 32rpx;
  top: 21rpx;
  right: 102rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 32rpx 32rpx;
}
.reduce_num_down{
  position: absolute;
   box-sizing: border-box;
  width: 32rpx;
  height: 32rpx;
  top: 21rpx;
  right: 102rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 32rpx 32rpx;
}

.num {
   box-sizing: border-box;
  position: absolute;
  width: 52rpx;
  height: 32rpx;
  line-height:36rpx;
  text-align: center;
  font-size: 24rpx;
  color: #333;
  top: 21rpx;
  right:42rpx;
}

.add_num {
  box-sizing: border-box;
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  top: 21rpx;
  right: 0rpx;
   background: url(data:image/png;base64,) no-repeat -1px center;
  background-size: 32rpx 32rpx;
}
.add_num_down{
  box-sizing: border-box;
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  top: 21rpx;
  right: 0rpx;
   background: url(data:image/png;base64,) no-repeat -1px center;
  background-size: 32rpx 32rpx;
}
.goods_detail {
  padding-top: 54rpx;
}

.detail {
  margin-top: 20rpx;
  /* margin-top: 24rpx; */
  font-size: 28rpx;
  color: #666;
  display: flex;
  flex-direction: row;
}

.detail view {
  flex: 1;
}

.detail_text {
  text-align: left;
}
.detail_num {
  text-align: right;
  color: #333;
}
.num_sign {
  font-size: 20rpx;
}
.num_sign_2 {
  font-size: 20rpx;
  color: #333;
  font-weight: bold;
}

.detail_num_1 {
  color: #d81c22;
  font-weight: bold;
}

.detail_num_2 {
  color: #333;
  font-weight: bold;
}

.cancel_sure {
  margin-top: 40rpx;
  text-align: right;
}

.cancel {
  display: inline-block;
  border: 1px solid #999;
  font-size: 28rpx;
  color: #999;
  border-radius: 52rpx;
  width: 140rpx;
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  margin-right: 30rpx;
  }

.sure {
  display: inline-block;
  width: 140rpx;
  height: 52rpx;
  line-height: 52rpx;
  border: 1px solid #999;
  font-size: 28rpx;
  color: #999;
  border-radius: 52rpx;
  text-align: center;
}
.sureactive{
 color: #fff;
 border: 1px solid #db1e35;
 background: #db1e35;
}
