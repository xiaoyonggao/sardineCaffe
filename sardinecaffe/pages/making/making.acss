.groupnum {
  text-align: center;
  padding: 30px 0;
  color: #fff;
}

.group {
  width: 60%;
  background: #181c20;
  height: 3px;
  margin: 0 auto;
  position: relative;
}

.group view {
  width: 62%;
  height: 3px;
  background: #3b9c59;
  transition: all 0.1s ease;
}

.t {
  text-align: center;
  font-size: 32rpx;
  color: #fff;
}

.makeGif {
  width: 120rpx;
  display: block;
  margin: 10px auto;
}

.gifBox {
  width: 150rpx;
  height: 150rpx;
  margin: 10px auto;
  overflow: hidden;
  position: relative;
}

.gifBox image {
  width: 50rpx;
  display: block;
  animation: moves 1.8s linear infinite;
  position: absolute;
  right: -50rpx;
  top: 15px;
}

@keyframes moves {
  0% {
    transform: translate3d(0, 0, 0);
  }

  100% {
    transform: translate3d(-210rpx, 0, 0);
  }
}



.makeTipBox {
  color:#999;
 margin-left: 34rpx;
 padding-left: 46rpx;
 font-size:30rpx;
 background: url(data:image/png;base64,) no-repeat;
 background-size: 34rpx 36rpx;
}
.product_tip{
  width: 100%;
  position: absolute;
  bottom: 60rpx;
  left: 0px;
  text-align: center;
  color: 999;
  font-size: 24rpx;
}