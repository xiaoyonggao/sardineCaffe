@import "../../templates/templeteToast/templeteToast.acss";
.wrap_in {
  padding-left: 100rpx;
}

.title {
  padding-top: 60rpx;
  font-size: 40rpx;
  color: #333;
  font-weight: bold;
}

.title text {
  display: block;
  padding-top: 7px;
  font-size: 34rpx;
  font-weight: bold;
}

.nowUser {
  margin-top: 38rpx;
  overflow: hidden;
}

.avatar {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
  float: left;
}
.avatar image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}
.in_production {
  float: left;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #333;
  font-size: 30rpx;
  margin-left: 20rpx;
  transition: all 3s ease-out;
}
.production_gif {
  width: 550rpx;
  height: 550rpx;
  opacity: 1;
}
.pro_animation{
  opacity: 0;
  transition: all .5s ease-out;
}
.production_gif image {
  width: 100%;
  height: 100%;
}
.product_model {
  clear: both;
  margin-top: 66rpx;
  position: relative;
}
.makeImg_model {
  width: 550rpx;
  height: 550rpx;
  position: absolute;
  top: 350rpx;
  left: 0rpx;
  opacity: 0;
 }
.make_animation{
  opacity: 1;
  top: 0rpx;
  left: 0rpx;
  transition: all .5s ease-out;
}
.makeImg_model image {
  width: 100%;
  height: 100%;
}




