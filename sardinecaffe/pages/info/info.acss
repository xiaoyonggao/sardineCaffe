.select_out {
  padding: 0rpx 100rpx;
}
.wrap{
height:100%;
overflow: hidden;
}
.topBox {
  color: #333;
  box-sizing: border-box;
  padding-top: 52rpx;
  text-align: left;
}

.cnname text {
  display: block;
}

.cnname .name {
  float: left;
  font-size: 40rpx;
  color: #333;
  font-weight: bold;
}

.cnname .ml {
  float: left;
  font-size: 24rpx;
  color: #666;
  margin-top: 20rpx;
  margin-left: 20rpx;
}

.enname {
  clear: both;
  font-size: 34rpx;
  margin-top: 4rpx;
  font-weight: bold;
}

.cool_hot {
  transition: all 0.2s ease;
  font-size: 30rpx;
  border-radius: 3px;
  width: 344rpx;
  margin: 0 auto;
  clear: both;
  overflow: hidden;
  border: 1px solid #fff;
  margin-top: 114rpx;
  margin-bottom: 80rpx;
}

.cool_hot text {
  padding: 0px 0rpx 4rpx 0rpx;
  color: #333;
  font-size: 28rpx;
  font-weight: bold;
}

.cool_hot text.active, .cool_hot text:active {
  border-bottom: 4rpx solid #db1e35;
}

.cool {
  float: left;
}

.hot {
  float: right;
}

.clean {
  clear: both;
  color: #333;
}

.icon_left {
  position: absolute;
  width: 52rpx;
  height: 31rpx;
  transform: rotate(-180deg);
  left: -63rpx;
  top: 4rpx;
}

.icon_right {
  position: absolute;
  width: 52rpx;
  height: 31rpx;
  right: -63rpx;
  top: 4rpx;
}

.maginTop {
  padding-top: 128rpx;
}

.item {
  margin-bottom: 50rpx;
  box-sizing: border-box;
}

.item .title {
  font-size: 28rpx;
}

.item .title image {
  float: left;
  position: relative;
  margin-right: 15rpx;
}

slider {
  margin: 0px;
}

.slideBox {
  font-size: 28rpx;
  position: relative;
  padding: 12rpx 100rpx 0rpx 100rpx;
}

.slideBox .left {
  position: absolute;
  left: 54rpx;
  top: 16rpx;
}

.slideBox .right {
  position: absolute;
  right: 53rpx;
  top: 16rpx;
}

.step_tit {
  margin-top: 7px;
}

.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}

.clearfix {
  zoom: 1;
}

.step_tit view {
  width: 20%;
  float: left;
  position: relative;
  text-align: center;
  font-size: 28rpx;
}

.step_tit view text.l {
  display: block;
  text-align: left;
  position: relative;
  left: -2px;
}

.step_tit view text.r {
  display: block;
  text-align: right;
  position: relative;
  right: -2px;
}

.af text, .af1 text, .af2 text {
  opacity: 0;
}

.wrap.blur {
  filter: blur(4px);
}

