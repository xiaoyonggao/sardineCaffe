.step_tit view.left1 {
  text-align: left;
}

.step_tit view.right1 {
  text-align: right;
}

.coupon {
  padding: 10px 0 10px 0;
  text-align: center;
  color: #333;
  font-size: 24rpx;
  margin-bottom: 16rpx;
}

.buttonBox {
  position: absolute;
  box-sizing: border-box;
  z-index: 10;
  color: #fff;
  font-size: 30rpx;
  overflow: hidden;
  width: 100%;
  left: 0px;
  bottom: 40rpx;
  padding: 0 14px;
}

.buttonBoxisIpx {
  bottom: 108rpx !important;
}

.btn1 {
  transition: all 0.2s ease;
  color: #db1e35;
  text-align: center;
  height: 94rpx;
  font-size: 34rpx;
  line-height: 94rpx;
  padding: 0px 0px;
  border-radius: 50px;
  box-sizing: border-box;
  border: 1rpx solid #db1e35;
  width: 80%;
 
  margin: 0 auto;
}
.btn2 {
  transition: all 0.2s ease;
  color: #fff;
  background: #db1e35;
  height: 94rpx;
  line-height: 94rpx;
  text-align: center;
  font-size: 34rpx;
  padding: 0px 0;
  border-radius: 50px;
  border: 1rpx solid #db1e35;
  box-sizing: border-box;
  width: 80%;
  margin: 0 auto 20rpx auto;
}

.btn2:active {
  color: #fff;
}

.hart image {
  width: 44rpx;
  height: 37rpx;
}

.hart {
  padding: 10px;
  position: absolute;
  right: 0;
  top: 0;
  z-index: 10;
}

.addSc {
  position: fixed;
  left: 0px;
  top: 0px;
  bottom: 0;
  right: 0px;
  background: rgba(0, 0, 0, 0.7);
  z-index: 66;
}

.addSc .tit {
  color: #fff;
  font-weight: bold;
  text-align: center;
  font-size: 36rpx;
  margin-top: 200rpx;
}

.addSc .inp {
  width: 90%;
  display: block;
  margin: 25px auto;
  background: rgba(123, 122, 120, 0.8);
  position: relative;
  height: 100rpx;
  border-radius: 4px;
}

.closeBtn {
  position: absolute;
  right: 0px;
  width: 50px;
  top: 0px;
  z-index: 10;
  text-align: center;
  line-height: 110rpx;
  height: 100%;
}

.closeBtn:after {
  content: '';
  display: block;
  width: 60px;
  height: 80px;
  position: absolute;
  left: -5px;
  top: -16px;
}

.closeBtn image {
  width: 18px;
  height: 18px;
}

.addSc .inp input {
  color: #fff;
  padding: 0 0 0 10px;
  font-size: 28rpx;
  height: 100rpx;
  width: 80%;
}

.btnBox {
  text-align: center;
}

.btnBox text {
  display: inline-block;
  border-radius: 4px;
  margin: 0 10px;
  border: 1px solid #28a5e6;
  padding: 8px 25px;
  color: #28a5e6;
}

.btnBox text:active {
  background: #28a5e6;
  color: #fff;
}

.couponid {
  transition: all 0.2s ease;
  color: #db1e35;
  text-align: center;
  height: 94rpx;
  font-size: 34rpx;
  line-height: 94rpx;
  padding: 0px 0px;
  border-radius: 50px;
  box-sizing: border-box;
  border: 1rpx solid #db1e35;
  width: 80%;
  margin:0 auto 20rpx auto;
}
.q {
  background: #db1e35;
  border: 1px solid #db1e35;
  color: #fff;
}
.paytips{
  font-size:20rpx;
  color:#999;
  text-align: center;
  margin-bottom: 20rpx;
}
.coupon_tips{
  border-radius:16rpx;
  padding: 18rpx 0rpx;
  color: #db1e35;
  font-size: 24rpx;
}
