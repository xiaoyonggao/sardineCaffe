.wrap {
  padding: 40rpx 50rpx;
}

.refund_price {
  display: flex;
  flex-direction: row;
  padding-bottom: 16rpx;
  border-bottom: 1rpx solid #f2f2f2;
}

.refund_left {
  text-align: left;
  flex: 1;
  font-size: 28rpx;
  color: #333;
  line-height:48rpx;
}

.refund_right {
  text-align: right;
  flex: 1;
  font-size: 36rpx;
  color: #db1e35;
}

.right_sign {
  font-size: 20rpx;
}

.contian_phone {
  margin-top:18rpx;
  display: flex;
  flex-direction: row;
}

.phone_left {
  width:120rpx;
  font-size:28rpx;
  color:#333;
  height:56rpx;
  line-height:56rpx;

}
.phone_right_color{
margin-left:10rpx;
flex:1;
height:56rpx;
line-height:56rpx;
color:#999;
}
.phone_right {
 margin-left:10rpx;
flex:1;
height:56rpx;
line-height:56rpx;
color:#dedede;
}
.phone2_right{
  font-size:28rpx;
  text-align: right;
  color:#333;
  height:56rpx;
  line-height:56rpx;
}
.phone_right input,.phone_right_color input {
  outline: none;
  font-size:28rpx;
  padding-left: 10rpx;
  text-align: right;
  height:56rpx;
  line-height:56rpx;
  width: 98%;
}
.reason_wrap {
  margin-top: 60rpx;
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 56rpx;
  line-height: 56rpx;
}

.reason_left {
  width: 120rpx;
  font-size: 28rpx;
  color: #333;
  height: 56rpx;
  line-height: 56rpx;
}

.reason_right {
  margin-left: 10rpx;
  flex: 1;
  height: 56rpx;
  line-height: 56rpx;
  color:#666;
  font-size:22rpx;
}

.reason_right input {
  outline: none;
  border: 1px solid #e7e7e7;
  font-size:24rpx;
  padding-left: 10rpx;
  height: 56rpx;
  line-height: 56rpx;
  width: 95%;
}

.tip_out {
  margin-left: 130rpx;
  width: 522rpx;
  height: auto;
  box-sizing: border-box;
  margin-top: 38rpx;
}

.tip_text {
  font-size: 20rpx;
  color: #999;
}

.tip_in {
  padding: 28rpx;
  border-radius: 16rpx;
  overflow: hidden;
  box-sizing: border-box;
  vertical-align: top;
}
.up_image,.up_image2{
  display: inline-block;
  width: 140rpx;
  height: 140rpx;
  margin-right: 20rpx;
  border-radius: 16rpx;
  overflow: hidden;
  vertical-align: top;
  margin-top: 20rpx;
}
.up_image:last-child{
  background: #e7e7e7;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 72rpx 60rpx;
  background-position: center center;
}
.up_image image,.up_image2 image{
  width: 100%;
  height: 100%;
}

.up_image:nth-child(1),.up_image2:nth-child(1){
  margin-top: 0rpx;
}
.up_image:nth-child(2),.up_image2:nth-child(2){
  margin-top: 0rpx;
}
.up_image:nth-child(3),.up_image2:nth-child(3){
  margin-top: 0rpx;
}
.up_image:nth-child(3n+3),.up_image2:nth-child(3n+3){
  margin-right: 0rpx;
}
.refund_tip {
  margin-top: 183rpx;
  display: flex;
  flex-direction: row;
}

.refund_tip_left {
  font-size: 24rpx;
  color: #999;
  width: 104rpx;
  margin-right: 26rpx;
}

.refund_tip_right {
  flex: 1;
  font-size: 20rpx;
  color: #999;
}
.right_tip:first-child{
  margin-top: 2rpx;
}
.right_tip {
  position: relative;
  line-height:30rpx;
  margin-bottom: 20rpx;
  padding-left: 10rpx; 
}
.right_tip:after{
  position: absolute;
  content: '';
  width:8rpx;
  height:8rpx;
  top:11rpx;
  left: -4rpx;
  background:#666666;
  border-radius:32rpx;
}
.right_tip text{
  color:#666;
}
.up_submit{
  background:#db1e35;
  border-radius:30rpx;
  width:100%;
  height:60rpx;
  line-height: 60rpx;
  font-size:30rpx;
  color:#fff;
  text-align: center;
  margin-top: 70rpx;
}
.up_submit_down{
  border:1px solid #999;
  border-radius:30rpx;
  width:100%;
  height:60rpx;
  line-height: 60rpx;
  font-size:30rpx;
  color:#999;
  text-align: center;
  margin-top: 70rpx;
}
.up_submit2,.up_submit1{
  position: relative;
  border:1rpx solid #999999;
  border-radius:30rpx;
  width:100%;
  height:60rpx;
  line-height: 60rpx;
  font-size:30rpx;
  color:#999999;
  text-align: center;
  margin-top: 70rpx;
}
.up_submit1 text{
  margin-left: -30rpx;
}
.up_submit1:after{
  content: '...';
  position: absolute;
  top:-3px;
  right:265rpx;
}
.up_submit3{
  border:1rpx solid #db1e35;
  border-radius:30rpx;
  width:100%;
  height:60rpx;
  line-height: 60rpx;
  font-size:30rpx;
  color:#db1e35;
  text-align: center;
  margin-top: 70rpx;
}
