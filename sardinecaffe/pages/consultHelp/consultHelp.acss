@import "/wxParse/wxParse.acss";
.wrap {
  padding:0rpx 90rpx;
  padding-top:40rpx;
}

.help_wrap {
  font-size: 40rpx;
  color: #333;
  padding: 10rpx 20rpx;
  font-weight: bold;
}

.item-li {
  width: 100%;
  overflow: hidden;
  background: #fff;
}

.item-li .item-title {
  font-size: 30rpx;
  color: #333;
  padding: 20rpx 20rpx;
  position: relative;
  display: block;
}

.item-li .item-content {
  text-indent: 0px;
  background: #fff;
  padding: 20rpx 30rpx 20rpx 40rpx;
}
.triangle {
  width: 0;
  height: 0;
  border-width: 0 10rpx 10rpx;
  border-style: solid;
  border-color: transparent transparent #f2f2f2; /*透明 透明  灰*/
  position: relative;
  top: -74rpx;
}

.item-li .item-content view {
  position: relative;
  color: #666;
  font-size: 24rpx;
  margin-bottom: 20rpx;
}
.item-li .item-content view::after {
  position: absolute;
  display: inline-block;
  left: -20rpx;
  top:16rpx;
  content: "";
  background: #333;
  width: 10rpx;
  height: 10rpx;
  border-radius: 100%;
}
.item-li .item-title text {
  color: #000;
  font-size: 28rpx;
}
.arrow_down {
  display: inline-block;
  position: absolute;
  top: 34rpx;
  right: 30rpx;
  border-right: 3rpx solid #999;
  border-bottom: 3rpx solid #999;
  width: 15rpx;
  height: 15rpx;
  transform: rotate(-45deg);
  transition: all ease 0.5s;
}

.arrow_up {
  display: inline-block;
  position: absolute;
  top: 34rpx;
  right: 30rpx;
  border-right: 3rpx solid #999;
  border-bottom: 3rpx solid #999;
  width: 15rpx;
  height: 15rpx;
  transform: rotate(45deg);
  transition: all ease 0.5s;
}

.dotted {
  background: #333;
  width: 10rpx;
  height: 10rpx;
  border-radius: 100%;
}

.makePhoneCall {
  position: fixed;
  bottom: 0rpx;
  height: 98rpx;
  line-height: 98rpx;
  width: 100%;
  background: #f2f2f2;
  text-align: center;
  padding-left: 20rpx;
  font-size: 30rpx;
  color: #666;
}

.itemBg4 {
  background: url(data:image/png;base64,) 160rpx center no-repeat;
  background-size: 40rpx 40rpx;
}
.quetion{
  font-size:28rpx;
  color:#666666;
  font-weight: bold;
}
