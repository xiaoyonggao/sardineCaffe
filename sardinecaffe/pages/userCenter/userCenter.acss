.topSection{overflow: hidden; padding-bottom: 24rpx; background: -webkit-radial-gradient(rgba(52,29,3,1), rgba(12,7,1,1))}
.userImgBox{width: 140rpx; height: 140rpx; border-radius: 50%; position: relative; display: block;margin: 55rpx auto 10px auto}
.userImgBox image.uImg{width: 140rpx; height: 140rpx;  border-radius: 50%;overflow: hidden;}
.topSection{ text-align: center; color: #fff;font-size: 36rpx; }
.edit2{width: 46rpx; height: 36rpx; position: absolute; right: -4rpx; bottom: 2rpx;}

.cellBox{font-size: 26rpx;color: #fff;text-align: center; padding-top: 12rpx;}
.cellBox .viewCell{display: inline-block;margin: 0 20px;}

.cellBox .viewCell text{display: block;}
.n{margin-top: 1px;}
.n image{width: 30rpx; height: 36rpx; position: relative; top: 3px;}
.levBox{margin-top: -2px;}
.levBox image{width: 20rpx; height: 20rpx;margin-right: 4px;}
.tabCellBox{display: flex;margin-bottom:5px; background: #fff;}
.tabCellBox .linkView{display: block; flex: 1;color: #666666; text-align: center; padding: 25rpx 0;font-size: 22rpx;}
.tabCellBox image{width: 44rpx;height: 44rpx; display: block;margin:0px auto 6px auto;}

.itemCellBox{position: relative; height: 94rpx;font-size: 28rpx; line-height: 94rpx;background: #fff; border-bottom: 1px solid #f2f2f2 }
.itemCellBox text{ float: right;padding-right: 12px;  }
.itemCellBox.rec{ padding-right: 30px;}
.itemCellBox.rec:after{
	content: '';
	display: block;
	width: 7px;
    height: 7px;
    border: 1px solid #999999;
    -webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    transform: rotate(-45deg);
    border-top: none;
    border-left: none;
    position: absolute; right: 12px; top: 50%;margin-top: -5px;
}
